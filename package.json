{
  "author": "Alberto Terragni",
  "bugs": "https://github.com/ninjabachelor/markdown-yaml-metadata-parser/issues",
  "dependencies": {
    "js-yaml": "^3.13.1",
    "platform": "^1.3.5"
  },
  "description": "Parse YAML metadata in a markdown document",
  "devDependencies": {
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/*.js"
    ],
    "testPathIgnorePatterns": [
